<div id="about">
  <div class="about-content">
    <h1>This template was built with <i class="fa fa-heart"></i> by</h1>
    <ul class="list-inline">
      <% ["Fl00rtje", "Dabbous1"].each do |user| %>
        <li>
          <% link_to "https://kitt.lewagon.com/alumni/#{user}", target: "_blank" do %>
            <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user}", class: "img-circle" %>
          <% end %>
          <h2><%= user %></h2>
          <p>
            <% link_to "https://github.com/#{user}", target: "_blank" do %>
              <i class="fa fa-github"></i> <%= user %>
            <% end %>
          </p>
        </li>
      <% end %>
    </ul>
  </div>
</div>

